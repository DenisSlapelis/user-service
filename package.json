{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User microservice to Create, Read, Update and Delete Users.",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register .",
    "build": "tsc --p tsconfig.build.json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepretty-quick": "npx jest .",
    "pretty-quick": "npx pretty-quick --staged --verbose --pattern \"**/*.*(js|ts|json)\"",
    "prepare": "node -e \"process.env.NODE_ENV != 'production' && process.exit(1)\" || npx husky install",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/index.ts"
  },
  "author": "Denis Slapelis",
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.484.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.6.5",
    "reflect-metadata": "^0.2.1",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7-rc.0",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.8.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.10.6",
    "@types/sequelize": "^4.28.17",
    "@types/winston": "^2.4.4",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
